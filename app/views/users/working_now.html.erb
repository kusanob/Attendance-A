<% provide(:title, '出勤中社員一覧') %>
<h1>出勤社中員一覧</h1>
<div>
  <table class= "table table-bordered table-condensed index-table">
    
    <tr bgcolor="#f5f5f5">
      <td>社員番号</td>
      <td>名前</td>
    </tr>
    <% ary = [@now_users_employee_number, @now_users].transpose %>
    <% ary.each do |kye, value| %>
      <tr>
        <td><%= kye %></td>
        <td><%= value %></td>
        </tr>
    <% end %>
    
  </table>
</div>